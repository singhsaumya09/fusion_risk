{
  "name": "control-center",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "webpack-dev-server --env.env=dev",
    "dev:bundleanalyzer": "yarn dev --env.addons=bundleanalyzer",
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production webpack -p --env.env=prod",
    "build:bundleanalyzer": "yarn build --env.addons=bundleanalyzer",
    "forever:start": "forever start build-utils/server/dev-server.js",
    "forever:stop": "forever stop build-utils/server/dev-server.js",
    "build:dist": "better-npm-run build:dist",
    "build:prod": "better-npm-run build:prod",
    "start": "better-npm-run start",
    "webpack-start": "better-npm-run webpack-start",
    "test": "better-npm-run test",
    "build-prod": "better-npm-run build-prod",
    "build-staging": "better-npm-run build-staging"
  },
  "betterScripts": {
    "compile": {
      "command": "node bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "nodemon bin/dev-server --ignore dist --ignore coverage --ignore tests --ignore src",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "prod": {
      "command": "nodemon bin/dev-server --ignore dist --ignore coverage --ignore tests --ignore src",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run clean && npm run compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:dev": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "deploy:serve": {
      "command": "npm run deploy && npm start",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "start": {
      "command": "node dev-server --env.env=dev",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "build-prod": {
      "command": "cross-env NODE_ENV=production webpack -p --env.env=prod",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "build-staging": {
      "command": "cross-env NODE_ENV=development webpack -p --env.env=dev",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "test": {
      "command": "karma start",
      "env": {
        "NODE_ENV": "test"
      }
    }
  },
  "dependencies": {
    "axios": "^0.18.0",
    "axios-progress-bar": "^1.2.0",
    "body-parser": "^1.18.3",
    "bootstrap": "^3.3.7",
    "chalk": "^2.4.1",
    "cross-env": "^5.2.0",
    "express": "~4.15.2",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "forever": "^0.15.3",
    "jquery": "^3.3.1",
    "moment": "^2.23.0",
    "mysql": "^2.16.0",
    "node-sass": "^4.12.0",
    "postcss-loader": "^2.1.5",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-bootstrap": "^0.32.1",
    "react-delay-render": "^0.1.2",
    "react-dom": "^16.4.1",
    "react-hot-loader": "^4.3.3",
    "react-imported-component": "^4.6.1",
    "react-prop-types": "^0.4.0",
    "react-redux": "^5.0.7",
    "react-router": "^3.0.0",
    "redbox-react": "^1.6.0",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^7.0.3",
    "semantic-ui-react": "^0.81.1",
    "url-loader": "^1.0.1",
    "webpack-merge": "^4.1.3",
    "yargs": "^12.0.5"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "better-npm-run": "^0.1.0",
    "css-loader": "^0.28.11",
    "html-webpack-plugin": "^3.2.0",
    "style-loader": "^0.21.0",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "webpack-hot-middleware": "^2.22.3",
    "webpack-node-externals": "^1.7.2",
    "ws": "5.2.4"
  }
}
